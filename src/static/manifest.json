{
  "manifest_version": 3,
  "name": "Apate - Fighting Misinformation",
  "version": "1.0.0",
  "description": "Not sure if you can trust the information you're reading? Apate will quickly search for reputable sources to verify your claim.",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon32.png",
      "32": "images/icon32.png"
    },
    "default_title": "Apate: Verify a Claim",
    "default_popup": "popup.html"
  }
}